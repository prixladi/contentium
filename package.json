    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scroll-up": "^1.4.0",
    "tailwindcss": "^2.2.9"
  },
  "devDependencies": {
    "@types/node": "22.7.7",
    "@types/prismjs": "^1.16.6",
    "@types/react": "17.0.19",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
