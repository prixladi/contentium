{
  "name": "contentium-server",
  "version": "1.0.0",
  "description": "Server for contentium project.",
  "author": "Ladislav Prix",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "start": "node ./dist/server/src/index.js",
    "start:docker": "node ./dist/app/src/index.js",
    "format:check": "prettier -l \"**/*.{js,ts}\"",
    "format:fix": "prettier --write \"**/*.{js,ts}\"",
    "code:check": "yarn format:check",
    "code:fix": "yarn format:fix",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:addMigration": "sequelize-cli migration:generate --name",
    "db:addSeeder": "sequelize-cli seed:generate --name",
    "db": "sequelize-cli"
  },
  "dependencies": {
    "@adminjs/express": "^4.0.0",
    "@adminjs/sequelize": "^2.0.0",
    "adminjs": "^5.1.0",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-promise-router": "^4.1.0",
    "express-session": "^1.17.2",
    "pg": "^8.6.0",
    "sequelize": "^6.6.5",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.3.5"
  }
}
